create database tododb;

use tododb;

create table todoTable(
    todoTableId int auto_increment primary key,
    title varchar(30),
    isComplete boolean
);

insert into todoTable (title, isComplete) values ('work 1', 1);
insert into todoTable (title, isComplete) values ('work 2', 0);
insert into todoTable (title, isComplete) values ('work 3', 1);